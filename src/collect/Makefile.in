# ---------------------------------------------------------

CC               = @CC@
CFLAGS           = @CFLAGS@
CPPFLAGS         = @CPPFLAGS@
LD               = @LD@
LDFLAGS          = @LDFLAGS@

CPPFLAGS        += -I..

ifeq "@DEBUG@" "TRUE"
CPPFLAGS        += -DDEBUG
endif

CFLAGS          += -fPIC
CFLAGS          += $(HASH_CFLAGS)

.PHONY: all default clean

all default:	fcollect.o collect.o

clean:
	rm -f fcollect.o collect.o
