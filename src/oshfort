#!/bin/sh
#
# generated from OpenSHMEM src directory, don't edit installed version
#
SHMEM_LDFLAGS="-L/home/tonyc/SHMEM/lib -Wl,-rpath,/home/tonyc/SHMEM/lib"
SHMEM_LIB="-lopenshmem"
SHMEM_INCLUDE_DIR="/home/tonyc/SHMEM/include"
exec /opt/openmpi/gnu/1.4.3/bin/mpif90 -I$SHMEM_INCLUDE_DIR -DGASNET_PAR -D_REENTRANT -D_GNU_SOURCE -I/home/tonyc/gasnet/everything/include -I/home/tonyc/gasnet/everything/include/mpi-conduit -O3 --param max-inline-insns-single=35000 --param inline-unit-growth=10000 --param large-function-growth=200000 -Winline $@ $SHMEM_LDFLAGS $SHMEM_LIB -O3 --param max-inline-insns-single=35000 --param inline-unit-growth=10000 --param large-function-growth=200000 -Winline -L/home/tonyc/gasnet/everything/lib -lgasnet-mpi-par -lammpi -lpthread -lm
