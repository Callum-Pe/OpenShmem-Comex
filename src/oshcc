#!/bin/sh
#
# generated from OpenSHMEM src directory, don't edit installed version
#

SHMEM_LDFLAGS='-L/home/tonyc/SHMEM/lib -Wl,-rpath,/home/tonyc/SHMEM/lib'

SHMEM_LIBS='-lopenshmem'

SHMEM_INC_DIR='/home/tonyc/SHMEM/include'

GASNET_CPPFLAGS='  -DGASNET_PAR -D_REENTRANT -D_GNU_SOURCE   -I/home/tonyc/gasnet/everything/include -I/home/tonyc/gasnet/everything/include/mpi-conduit  '

GASNET_CFLAGS='-O3 --param max-inline-insns-single=35000 --param inline-unit-growth=10000 --param large-function-growth=200000 -Winline     '

GASNET_LDFLAGS='-O3 --param max-inline-insns-single=35000 --param inline-unit-growth=10000 --param large-function-growth=200000 -Winline    '

GASNET_LD='/opt/openmpi/gnu/1.4.3/bin/mpicc '

GASNET_LIBS='-L/home/tonyc/gasnet/everything/lib   -lgasnet-mpi-par -lammpi   -lpthread -lm '



exec $GASNET_LD -I$SHMEM_INC_DIR \
    $GASNET_CPPFLAGS $GASNET_CFLAGS $@ \
    $SHMEM_LDFLAGS $SHMEM_LIBS $GASNET_LDFLAGS $GASNET_LIBS
